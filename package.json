{
  "name": "reinicio_uni",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "prisma:validate:security": "powershell -NoProfile -Command \"$env:DATABASE_URL=$env:DATABASE_SECURITY_URL; npx prisma validate --schema=prisma/schema-security.prisma\"",
    "prisma:validate:academic": "powershell -NoProfile -Command \"$env:DATABASE_URL=$env:DATABASE_ACADEMIC_URL; npx prisma validate --schema=prisma/schema-academic.prisma\"",
    "prisma:validate:help": "powershell -NoProfile -Command \"$env:DATABASE_URL=$env:DATABASE_HELP_URL; npx prisma validate --schema=prisma/schema-help.prisma\"",
    "prisma:validate": "npm run prisma:validate:security && npm run prisma:validate:academic && npm run prisma:validate:help",
    "prisma:generate:security": "powershell -NoProfile -Command \"$env:DATABASE_URL=$env:DATABASE_SECURITY_URL; npx prisma generate --schema=prisma/schema-security.prisma\"",
    "prisma:generate:academic": "powershell -NoProfile -Command \"$env:DATABASE_URL=$env:DATABASE_ACADEMIC_URL; npx prisma generate --schema=prisma/schema-academic.prisma\"",
    "prisma:generate:help": "powershell -NoProfile -Command \"$env:DATABASE_URL=$env:DATABASE_HELP_URL; npx prisma generate --schema=prisma/schema-help.prisma\"",
    "prisma:generate": "npm run prisma:generate:security && npm run prisma:generate:academic && npm run prisma:generate:help",
    "migrate:dev:security": "powershell -NoProfile -Command \"$env:DATABASE_URL=$env:DATABASE_SECURITY_URL; npx prisma migrate dev --schema=prisma/schema-security.prisma\"",
    "migrate:dev:academic": "powershell -NoProfile -Command \"$env:DATABASE_URL=$env:DATABASE_ACADEMIC_URL; npx prisma migrate dev --schema=prisma/schema-academic.prisma\"",
    "migrate:dev:help": "powershell -NoProfile -Command \"$env:DATABASE_URL=$env:DATABASE_HELP_URL; npx prisma migrate dev --schema=prisma/schema-help.prisma\"",
    "migrate:dev:all": "npm run migrate:dev:security && npm run migrate:dev:academic && npm run migrate:dev:help",
    "db:setup": "npm run prisma:validate && npm run prisma:generate && npm run migrate:dev:all",
    "seed:security": "powershell -NoProfile -Command \"$env:DATABASE_URL=$env:DATABASE_SECURITY_URL; npx ts-node prisma/seeds/seed.security.ts\"",
    "seed:academic": "powershell -NoProfile -Command \"$env:DATABASE_URL=$env:DATABASE_ACADEMIC_URL; npx ts-node prisma/seeds/seed.academic.ts\"",
    "seed:help": "powershell -NoProfile -Command \"$env:DATABASE_URL=$env:DATABASE_HELP_URL; npx ts-node prisma/seeds/seed.help.ts\"",
    "seed:all": "npm run seed:security && npm run seed:academic && npm run seed:help",
    "db:setup:all": "npm run prisma:generate && npm run migrate:dev:all && npm run seed:all"
  },
  "dependencies": {
    "@nestjs/common": "^11.1.12",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.12",
    "@nestjs/jwt": "^11.0.2",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.1.12",
    "@nestjs/swagger": "^11.2.5",
    "bcrypt": "^5.1.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.3",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "pg": "^8.17.1",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.18.0",
    "@nestjs/cli": "^11.0.15",
    "@nestjs/schematics": "^11.0.9",
    "@nestjs/testing": "^11.1.12",
    "@prisma/client": "^7.0.1",
    "@types/bcrypt": "^5.0.2",
    "@types/express": "^5.0.6",
    "@types/jest": "^30.0.0",
    "@types/node": "^22.19.6",
    "@types/passport-jwt": "^4.0.1",
    "@types/supertest": "^6.0.2",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.2",
    "globals": "^16.0.0",
    "jest": "^30.0.0",
    "prettier": "^3.4.2",
    "prisma": "^7.0.1",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.4",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.20.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}